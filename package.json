{
  "name": "callai-e2e-automation",
  "version": "1.0.0",
  "description": "End 2 End Automation of CallAI platform",
  "main": "index.js",
  "scripts": {
    "open": "cypress open",
    "clean:reports": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
    "pretest": "npm run clean:reports",
    "lint": "eslint \"cypress/**/**/*.js\" --quiet --fix; echo \"🎉  Congrats! Your code is formatted correcty! 👍\" ; exit 0",
    "pretty": "prettier --write \"cypress/**/**/*.js\" ",
    "Format-Fix": "npm run pretty && npm run lint; exit 0",
    "scripts": "cypress run --browser chrome --headed",
    "headless-Run": "cypress run --headless",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test": "npm run scripts && npm run posttest"
  },
  "keywords": [
    "CallAI"
  ],
  "nyc": {
    "all": true,
    "report-dir": "cypress-coverage",
    "reporter": [
      "html"
    ],
    "extends": "@istanbuljs/nyc-config-typescript"
  },
  "author": "srewoo",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/preset-typescript": "7.14.5",
    "@cypress/browserify-preprocessor": "^3.0.1",
    "@cypress/instrument-cra": "^1.4.0",
    "@cypress/webpack-preprocessor": "5.9.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "axe-core": "^4.2.2",
    "babel-loader": "8.2.2",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "browserify-istanbul": "3.0.1",
    "chai": "^4.3.4",
    "check-code-coverage": "1.10.0",
    "cypress": "^8.2.0",
    "cypress-axe": "^0.8.2",
    "cypress-iframe": "^1.0.1",
    "cypress-istanbul": "^1.3.0",
    "cypress-mochawesome-reporter": "^2.2.0",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "istanbul-lib-coverage": "^3.0.0",
    "mocha": "^8.0.1",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "npm": "^10.2.2",
    "nyc": "^15.1.0",
    "prettier": "2.3.2",
    "prettier-eslint": "^13.0.0",
    "process": "0.11.10",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "cypress-image-snapshot": "^4.0.1",
    "cypress-plugin-snapshots": "^1.4.4",
    "@cypress/code-coverage": "^3.9.10"
  }
}
